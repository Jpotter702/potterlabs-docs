{
  "buildCommand": "bun run build",
  "outputDirectory": "out",
  "installCommand": "bun install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/guide/create-ai-captions",
      "destination": "/guide/create-ai-captions/index.html"
    }
  ]
}
